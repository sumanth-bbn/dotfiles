# Fig pre block. Keep at the top of this file.
[[ -f "$HOME/.fig/shell/bash_profile.pre.bash" ]] && builtin source "$HOME/.fig/shell/bash_profile.pre.bash"
# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/nagsumanth/opt/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/nagsumanth/opt/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/nagsumanth/opt/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/nagsumanth/opt/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

export PATH=/opt/homebrew/bin:$PATH
export PATH=/Users/nagsumanth/Library/Python/3.11/bin:$PATH
export PATH="$PATH:/Users/nagsumanth/.local/lib/python3.12/site-packages/cookiecutter"

export NVM_DIR=~/.nvm
source $(brew --prefix nvm)/nvm.sh

export PATH="$PATH:/Users/nagsumanth/flutter_sdk/flutter/bin" 

# Fig post block. Keep at the bottom of this file.
[[ -f "$HOME/.fig/shell/bash_profile.post.bash" ]] && builtin source "$HOME/.fig/shell/bash_profile.post.bash"
